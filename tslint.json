{
    "rules": {
        "class-name": true,
        "no-trailing-whitespace": false,
        "no-string-literal": true,
        "no-var-keyword": true,
        "eofline": true,
        "no-use-before-declare": false,
        "semicolon": true
    },
    "linterOptions": {
        "exclude": [
            "**/_*.*"
        ]
    }
}